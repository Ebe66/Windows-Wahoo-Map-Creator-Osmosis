<?xml version="1.0" encoding="UTF-8"?>
<rendertheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" map-background="#9f9c9a"
	version="1" xmlns="http://opensciencemap.org/rendertheme"
	xsi:schemaLocation="http://opensciencemap.org/rendertheme https://raw.githubusercontent.com/mapsforge/vtm/master/resources/rendertheme.xsd">

	<!--###### TRANSFORM tags ######-->
	<tag-transform k="roof:colour" k-lib="roof:colour" v="blue" v-lib="#001e80" />
	<tag-transform k="roof:colour" k-lib="roof:colour" v="green" v-lib="#007829" />
	<tag-transform k="roof:colour" k-lib="roof:colour" v="red" v-lib="#4d0000" />
	<tag-transform k="roof:colour" k-lib="roof:colour" v="yellow" v-lib="#857500" />
	<tag-transform k="building:colour" k-lib="building:colour" v="blue" v-lib="#000f6c" />
	<tag-transform k="building:colour" k-lib="building:colour" v="green" v-lib="#006400" />
	<tag-transform k="building:colour" k-lib="building:colour" v="red" v-lib="#6c0000" />
	<tag-transform k="building:colour" k-lib="building:colour" v="yellow" v-lib="#948200" />

	<!--###### TEXT styles ######-->

	<!--default label-->
	<style-text caption="true" dy="20" fill="#00005c" font-family="medium" id="caption-small-blue"
		k="name" size="14" stroke="#000000" stroke-width="2.0" />
	<!--shop-->
	<style-text caption="true" dy="20" fill="#4c004c" font-family="medium" id="shop" k="name"
		size="14" stroke="#000000" stroke-width="2.0" />
	<!--tavern-->
	<style-text caption="true" dy="20" fill="#130000" font-family="medium" id="tavern" k="name"
		size="14" stroke="#000000" stroke-width="2.0" />
	<!--accommodation-->
	<style-text caption="true" dy="20" fill="#00327a" font-family="medium" id="accommodation"
		k="name" size="14" stroke="#000000" stroke-width="2.0" />
	<!--education-->
	<style-text caption="true" dy="20" fill="#000000" font-family="medium" id="education" k="name"
		size="14" stroke="#000000" stroke-width="2.0" />
	<!--health-->
	<style-text caption="true" dy="20" fill="#7a0032" font-family="medium" id="health" k="name"
		size="14" stroke="#000000" stroke-width="2.0" />
	<!--entertainment-->
	<style-text caption="true" dy="20" fill="#570500" font-family="medium" id="entertainment"
		k="name" size="14" stroke="#000000" stroke-width="2.0" />
	<!--roads-->
<!--	<style-text fill="#efefef" id="road" k="name" priority="2" size="24" stroke="#111111"-->
<!--		stroke-width="2.0" font-family="sans_serif" />-->
	<!--major roads-->
<!--	<style-text style="bold" fill="#efefef" id="major-road" k="name" priority="1" size="24"-->
<!--		stroke="#111111" stroke-width="2.0" font-family="sans_serif" />-->
	<!--references-->
	<style-text style="bold" fill="#9f9f9f" id="ref" k="ref" priority="2" size="12" stroke="#000000"
		stroke-width="2.0" />
	<style-text caption="true" style="bold" fill="#000000" id="ref-caption" k="ref" priority="2"
		size="12" bg-fill="#004916" />
	<!--ferry-->
	<style-text style="bold" fill="#9f9f9f" id="ferry" k="name" size="12" stroke="#000000"
		stroke-width="2.0" />


	<!--###### AREA styles ######-->

	<!--residential-->
	<style-area fade="11" fill="#888783" id="residential" />
	<!--railway-->
	<style-area fade="10" fill="#868385" id="railway|industrial" />

	<!--Note: ordered according to their inheritance via the `use` attribute-->

	<!-- Avoid patterns and textures because of old GPU issues -->
	<!--<style-area id="tex1" src="assets:textures/rough.png" />-->

	<!-- meadow|garden -->
	<style-area fade="10" fill="#697c31" id="greens" />

	<!-- grass -->
	<style-area fade="12" fill="#58723e" id="lightgreen" />

	<!-- <style-area id="greens" fill="#72856e" fade="10" /> -->

	<!--farmland-->
	<style-area fade="12" fill="#847a55" id="farmland" />

	<!-- marsh|wetland|mud|nature_reserve -->
	<style-area fade="12" fill="#7e8c59" id="greens2" />

	<!-- park|common|green|cemetery|golf_course|dog_park -->
	<style-area fade="11" fill="#446e1c" id="park" />
	<!-- <style-area id="park" fill="#436a1b" fade="11" /> -->

	<!--wood-->
	<!-- fade out at z=7, blend over to 'blend-fill' in z=11 -->
	<!-- src="assets:textures/wood.png" -->
	<!-- <style-area id="wood" fill="#717b67" fade="8" blend="11" blend-fill="#3a650e" /> -->
	<!-- <style-area id="wood" use="park" fill="#3a650e" fade="8" /> -->
	<!-- B8D29E -->
	<style-area blend="11" blend-fill="#58723e" fade="8" fill="#537035" id="wood" use="park" />
	<!-- <style-line id="wood" fix="true" cap="butt" width="1.0" stroke="#3a650e" /> -->

	<!-- de:Kleingartengebiet -->
	<style-area fade="12" fill="#8f8a80" id="allotments" />

	<!-- de:Steinbruch, Schotter-, Kies-, Sand- und Tongrube -->
	<style-area fade="10" fill="#7d7d6c" id="quarry" />

	<!--military-->
	<style-area fade="10" fill="#8e8d8a" id="military" />
	<!--building-->
	<style-area fade="15" fill="#92908b" id="building" />

	<!--water-->
	<!--<style-area id="water" fill="#375785" afc5e3 /> -->
	<!-- src="assets:textures/water.png" -->
	<style-area fade="-1" fill="#003b84" id="water" />

	<!--###### LINE styles ######-->

	<!-- base style for fixed width lines -->
	<style-line cap="butt" fix="true" id="fix" width="1.0" />

	<!--residential-->
	<style-line id="residential" stroke="#555555" width="2.0" />
	<style-line id="residential:bridge" cap="butt" use="residential" />

	<!-- when inheriting another style with 'from' then 'width' is meant relative to the parent -->
	<style-line id="pedestrian" use="residential" width="-0.4" />
	<style-line id="pedestrian:bridge" cap="butt" use="pedestrian" />
	<style-line id="motorway" stroke="#AA5500" width="2.0" />
	<style-line id="motorway_link" cap="butt" stroke="#AA5500" width="1.3" />
	<style-line id="trunk_link" cap="butt" stroke="#9d915d" width="1.3" />
	<style-line id="trunk" stroke="#DD8800" width="3.5" />
	<style-line id="primary" stroke="#DDDD00" width="3.4" />
	<style-line id="secondary" stroke="#AAAAAA" width="3.0" />
	<style-line id="tertiary" stroke="#777777" width="2.2" />
	<style-line id="construction" stroke="#1f1f1f" width="1.2" />
	<style-line id="highway-service" stroke="#686868" width="0.8" />

	<!-- track|footway|path|cycleway -->
	<style-line cap="butt" fix="true" id="footway" stroke="#240000" width="1.5" dasharray="14,5" />
	<style-line cap="butt" fix="true" id="cycleway" stroke="#00621a" width="0.1" />

	<style-line id="highway:cycleway" use="cycleway" width="2" />
	<style-line id="highway:bicycle_shared" cat="shared_bike" use="cycleway" width="1.0" dasharray="25,10" />
	<style-line id="highway:track" stipple="3" stipple-stroke="#433b08" use="footway" width="0.1" />
	<style-line id="highway:path" stipple="2" stipple-stroke="#231b00" use="footway" width="0.1" />

	<!-- <style-line id="footway:z16" use="footway" width="-0.95" fixed="false" fade="-1"/> -->
	<style-line id="footway:z17" stroke="#9a9895" width="0.3" />

	<!-- de: ein Weg der fÃ¼r Reiter vorgesehen ist.. -->
	<style-line cap="butt" id="bridleway" stipple-stroke="#231b00" stroke="#736b38" use="footway"
		width="0.1" />
	<style-line cap="butt" fix="true" id="steps" stipple="2" stipple-stroke="#5e0200"
		stipple-width="1.0" stroke="#4a9f9fff" width="4.2" />
	<style-line cap="butt" fix="true" id="water:outline" stroke="#445b6c" width="1.0" />

	<!-- no-go area boundary -->
	<style-line cap="butt" fix="true" id="fence" stroke="#bbbbbb" width="1.2" />
	<style-line cap="butt" id="aeroway:runway" stroke="#686c5e" width="1.8" />

	<!--park-->
	<style-line cap="butt" fade="14" fix="true" id="park" stroke="#3a650e" width="1.0" />

	<!--building-->
	<!-- <style-line id="building" stroke="#696361" width="1.0" fix="true" cap="butt" fade="15"/> -->
	<!-- <style-line id="building" stroke="#706e68" width="1.0" fix="true" cap="butt" fade="15"/>
	  <style-area id="building" fill="#898683" fade="15"/> -->
	<style-line fade="15" id="building" stroke="#8e5756b3" use="fix" />

	<!--water-->
	<style-line cap="butt" fix="true" id="water" stroke="#003b84" width="1.0" />
	<style-line fix="false" id="river" stroke="#003b84" use="water" />

	<!--###### SYMBOL styles ######-->

	<!-- oneway -->
	<style-symbol id="oneway" repeat="true" src="assets:symbols/oneway.svg" />


	<!--###### ASSIGNMENT ######-->

	<m e="way" k="natural" v="issea|sea">
		<area mesh="true" use="water" />
	</m>

	<m e="way" k="natural" v="nosea">
		<area fill="#000000" mesh="true" />
	</m>

	<!-- all closed ways that are not 'highway' or 'building' -->

	<m closed="yes" e="way" k="highway|building" v="~">

		<!-- landuse base -->
		<m k="landuse">
			<m v="urban">
				<area fill="#949390" />
			</m>

			<m v="meadow|conservation">
				<area fade="11" use="greens" />
			</m>

			<m v="residential|commercial|retail|farmyard">
				<area use="residential" />
			</m>
		</m>
		<m k="natural" v="grassland|scrub">
			<area use="lightgreen" />
		</m>

		<m k="landuse">
			<m v="farmland|farm|orchard|vineyard|greenhouse_horticulture|plant_nursery">
				<area use="farmland" />
			</m>
			<m v="quarry">
				<area use="quarry" />
			</m>
			<!-- <m v="farmyard|retail|commercial|industrial;retail">
			  <area use="residential" />
			  </m> -->
			<m v="industrial|railway">
				<area use="railway|industrial" />
			</m>
			<!-- <m v="military">
			  <area use="military"/>
			  </m> -->
			<!--<m k="landuse" v="construction|greenfield"> <area fill="#441c00"
			  stroke="#1b1b1b" width="0.2" /> </m> -->
			<!-- <m k="landuse" v="garages"> <area fill="#767684" /> </m> -->
		</m>
		<m k="landuse|natural|leisure|amenity|tourism">
			<!-- kind of more like landuse imho -->
			<m k="leisure|landuse" v="nature_reserve">
				<area fill="#004b829c" />
				<m zoom-min="14">
					<line cap="butt" fix="true" stroke="#4b823c" width="1.0" />
				</m>
			</m>

			<!-- tourism areas
			  Berlin Zoologischer Garten has lots of details that should be drawn above
			-->
			<m k="tourism">
				<!-- <m k="tourism" v="attraction"> <area fill="#926a8a" /> </m> -->

				<m v="zoo|picnic_site|caravan_site|camp_site">
					<area fill="#60763a" />
				</m>
			</m>

			<!-- amenity -->
			<m k="amenity" zoom-min="14">
				<m v="kindergarten|school|college|university">
					<!-- <area fill="#6d4b7e" /> -->
					<area fade="14" fill="#847f5f" />
					<line cap="butt" fade="14" fix="true" stroke="#3a4b4e" width="1.0" />
				</m>
				<m v="hospital">
					<area fill="#927951" />
				</m>
				<!-- <m v="parking" zoom-min="15">
				  <area fill="#0b0b0b" />
				  </m>
				  <m v="fountain" closed="yes">
				  <area fill="#546b7c" />
				  </m> -->
			</m>

			<!-- landuse -->
			<m k="landuse" zoom-min="11">
				<!-- how about 'leisure' for this one? -->
				<m v="cemetery">
					<area use="park" />
					<m zoom-min="14">
						<line use="park" />
					</m>
				</m>
			</m>
			<m k="landuse" v="village_green|recreation_ground">
				<area use="greens" />
			</m>
			<m k="landuse" v="allotments" zoom-min="12">
				<area use="allotments" />
			</m>
			<m k="leisure" v="park|common|green|golf_course" zoom-min="11">
				<area use="park" />
				<m zoom-min="14">
					<line use="park" />
				</m>
				<!-- <text use="park" /> -->
			</m>

			<!-- Heideland, keep below forest -->
			<m v="heath|sand" zoom-min="10">
				<area fade="10" fill="#9f9a71" />
			</m>

			<m k="landuse|natural" v="forest|wood">
				<m zoom-min="8">
					<area use="wood" />
				</m>
			</m>

			<!-- keep grass above forest:wood and leisure:park! -->
			<!-- http://wiki.openstreetmap.org/wiki/Proposed_features/conservation,
			  often serves as background for leisure=nature_reserve -->
			<m k="landuse" v="grass">
				<area use="lightgreen" />
			</m>

			<m k="leisure" v="garden">
				<area use="greens" />
			</m>

			<!-- amenity -->
			<m k="amenity">
				<!--<m v="kindergarten|school|college|university" zoom-min="15">
				  <area fill="#6d5b6a" fade="15"/>
				  <line stroke="#3a4b4e" width="1.0" fix="true" cap="butt" fade="15"/>
				  </m>

				  <m v="hospital" zoom-min="14">
				  <area fill="#868465" />
				  </m>
				-->
				<m v="parking" zoom-min="15">
					<area fill="#0b0b0b" />
				</m>
				<m closed="yes" v="fountain">
					<area fill="#546b7c" />
				</m>

			</m>

			<!-- <m k="natural" v="coastline">
			  <line stroke="#445b6c" width="1.2" fix="true" />
			  </m> -->

			<!-- natural -->
			<m k="natural" zoom-min="10">
				<m v="glacier">
					<area fill="#9a9a9f" />
				</m>
				<m v="land">
					<area fill="#070707" />
				</m>
				<m v="beach">
					<area fill="#979568" />
				</m>
				<m v="marsh|wetland|mud">
					<area use="greens2" />
				</m>
			</m>

			<!-- leisure -->
			<m k="leisure" zoom-min="13">

				<m v="stadium">
					<line cap="butt" fix="true" stroke="#696361" width="1.0" />
					<area fill="#898683" />
				</m>

				<!--should be under playing field -->
				<m v="sports_centre|water_park" zoom-min="14">
					<area fade="12" fill="#7a8f7b" />
				</m>
				<m v="playground|miniature_golf" zoom-min="15">
					<area fill="#517d30" />
					<line cap="butt" fix="true" stroke="#797943" width="1.0" />
				</m>
				<m v="playing_fields|pitch">
					<area fill="#517d30" />
					<line cap="butt" fix="true" stroke="#797943" width="1.0" />
				</m>
				<m v="swimming_pool">
					<area fill="#748b9c" />
				</m>

				<!-- <m v="track"> <m k="area" v="yes|true">
				  <area fill="#697842" stroke="#576630" width="0.025" /> </m> <m
				  e="way" k="area" v="~|no|false"> <line stroke="#576630" width="0.75"
				  /> </m> </m> -->
			</m>

			<!-- area outlines need to be above to avoid uggly pixelation where
			  not aliased polygon overlaps the lines... -->
			<m k="leisure|landuse" zoom-min="14">

				<m v="nature_reserve">
					<line cap="butt" fix="true" stroke="#4b823c" width="1.0" />
				</m>
				<m v="military">
					<line use="fence" />
				</m>
			</m>
			<m k="landuse" v="reservoir|basin">
				<area use="water" />
			</m>

			<!-- ...should rewrite tag to: highway=leisure/sport=* imho -->
			<m k="leisure" v="track">
				<line cap="butt" fix="true" stroke="#615c56" width="1.3" />
			</m>
		</m><!-- end landuse|natural|leisure||amenity|tourism -->
	</m>

	<!-- waterways -->
	<m e="way" k="waterway">
		<m v="ditch|drain" zoom-min="14">
			<line fade="14" use="water" width="0.2" />
		</m>
		<m v="canal">
			<line use="river" width="-0.3" />
		</m>
		<m v="stream" zoom-min="13">
			<line fade="13" use="water" width="0.5" />
		</m>
		<m select="first" v="river">
			<m zoom-min="12">
				<line use="river" width="0.3" />
			</m>
			<!-- zoom <= 11 -->
			<m k="rank" v="~|-1" zoom-min="9">
				<line fade="9" use="water" width="0.2" />
			</m>
			<m k="rank" v="0|1|2">
				<line fade="2" use="water" width="0.3" />
			</m>
			<m k="rank" v="3" zoom-min="3">
				<line fade="3" use="water" width="0.3" />
			</m>
			<m k="rank" v="4" zoom-min="4">
				<line fade="4" use="water" width="0.3" />
			</m>
			<m k="rank" v="5" zoom-min="5">
				<line fade="5" use="water" width="0.3" />
			</m>
			<m k="rank" v="6" zoom-min="6">
				<line fade="6" use="water" width="0.2" />
			</m>
			<m k="rank" v="7" zoom-min="7">
				<line fade="7" use="water" width="0.2" />
			</m>
			<m k="rank" v="8" zoom-min="8">
				<line fade="8" use="water" width="0.1" />
			</m>
			<m k="rank" v="9" zoom-min="9">
				<line fade="9" use="water" width="0.1" />
			</m>
		</m>
		<m v="riverbank|dock">
			<area use="water" />
			<line use="water:outline" />
		</m>
		<m v="weir">
			<line stroke="#000028" use="fix" />
		</m>
		<m v="dam" zoom-min="12">
			<line stroke="#545454" use="fix" width="0.2" />
		</m>
		<m k="lock" v="yes|true">
			<line stroke="#070707" use="fix" width="0.5" />
		</m>

		<text fill="#000000" k="name" size="14" stroke="#4a9f9fff" stroke-width="2.0" />
	</m>

	<m e="way">
		<m closed="yes" k="natural" v="water">
			<area use="water" />
			<caption area-size="0.2" fill="#000000" k="name" size="16" stroke="#4a9f9fff"
				stroke-width="2.0" />
			<!--<line use="water:outline" />-->
		</m>

		<!-- sport -->
		<!-- <m k="sport"> <m k="sport" v="soccer"
		  zoom-min="17"> <m k="sport" v="swimming|canoe|diving|scuba_diving">
		  <area fill="#546b7c" stroke="#00009f" width="0.2" /> </m> <m
		  e="way" k="sport" v="tennis"> <area fill="#712a0a" stroke="#530c00" width="0.2"
		  /> </m> </m> -->

		<!-- outline 0 -->
		<outline-layer blur="1.0" id="glow" stroke="#ffffff" width="0.2" />
		<outline-layer id="0" stroke="#00000000" width="0.1" />

		<!-- match tunnel-tag (to ensure tunnel key is present) -->
		<m k="tunnel" zoom-min="11">
			<!-- match tunnel-tag that are not 'no' or 'false' -->
			<m k="tunnel" v="-|no|false">
				<!-- match area-tag that are 'no' or 'false' or not present -->
				<m k="area" v="~|no|false">
					<!-- highway tunnels -->
					<m k="highway">
						<m zoom-min="16">
							<m v="steps">
								<line use="steps" />
								<!-- <outline use="0"/> -->
							</m>
							<!-- <m v="track|footway|path|cycleway" zoom-min="16">
							  <line use="footway:z16"/>
							  </m> -->
						</m>
						<m zoom-min="14">
							<m v="footway">
								<line use="footway" />
							</m>
							<m v="cycleway">
								<line use="highway:cycleway" />
							</m>
							<m v="track">
								<line use="highway:track" />
							</m>
							<m v="path">
								<line use="highway:path" />
							</m>
							<m v="bridleway">
								<line use="bridleway" />
							</m>
							<m v="construction">
								<outline use="0" />
								<line outline="0" use="construction" />
							</m>
							<m v="service">
								<m k="service" v="~|parking_aisle">
									<line outline="0" use="highway-service" />
								</m>
							</m>
						</m>
						<m v="trunk_link|motorway_link">
							<line outline="0" use="trunk_link" />
							<text use="major-road" />
						</m>
						<m zoom-min="13">
							<m v="byway|pedestrian">
								<line outline="0" use="pedestrian" />
								<text use="road" />
							</m>
							<m v="residential|road|unclassified|living_street">
								<line outline="0" use="residential" />
								<text use="road" />
							</m>
						</m>
						<m v="tertiary|secondary_link">
							<line outline="0" use="tertiary" />
							<text use="road" />
						</m>
						<m v="secondary|primary_link">
							<line outline="0" use="secondary" />
							<text use="major-road" />
						</m>
						<m v="primary">
							<line outline="0" use="primary" />
							<text use="major-road" />
							<m zoom-max="12">
								<text use="ref-caption" />
							</m>
							<m zoom-min="13">
								<text use="ref" />
							</m>
						</m>
						<m v="trunk">
							<line blur="0.3" outline="0" use="trunk" />
							<!-- <outline use="glow"/> -->
							<text use="major-road" />
							<m zoom-max="12">
								<text use="ref-caption" />
							</m>
							<m zoom-min="13">
								<text use="ref" />
